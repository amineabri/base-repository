    "type": "library",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Amine Abri",
            "email": "amineabri@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
